<%= form_for @user do |f| %>
  <%= f.label :fname, "First Name" %>
  <%= f.text_field :fname %>
  <%= f.label :lname, "Last Name" %>
  <%= f.text_field :lname %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %> 
  <%= f.submit %>
<% end %>